<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lozan</title>
    <link rel="stylesheet" href="~/assets/images/favicon.ico" />
    @RenderSection("Styles", false)
    <link rel="stylesheet" href="~/assets/css/bootstrap-dark.min.css" />
    <link rel="stylesheet" href="~/assets/css/icons.min.css" />
    <link rel="stylesheet" href="~/assets/css/app-dark.min.css" />
    <link rel="icon" type="image/png" href="~/Images/favicon-16x16.png" />
</head>
<body>
    <!-- Begin page -->
    <div id="wrapper">

        <!-- Topbar Start -->
        @*@await Html.PartialAsync("_TopBar")*@
        @{ await Html.RenderPartialAsync("_TopBar");}
        @*<partial name="~/Shared/_TopBar.cshtml" />*@

        <!-- ========== Left Sidebar Start ========== -->
        @await Html.PartialAsync("_SideMenu")
        <!-- Left Sidebar End -->
        <div class="content-page">
            <div class="content">
                <!-- Start Content-->
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            <div class="text-md-center footer-links d-none d-sm-block">
                &copy; 2021 - Lozan - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </div>
    </footer>

    <script src="~/assets/js/vendor.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="~/assets/js/app.min.js"></script>
</body>
</html>
